{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "target": "es2015",
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "types": [
      "googlemaps",
      "node"
    ],
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@nebular/*": [
        "../node_modules/@nebular/*"
      ],
      "@classes/*": [
        "src/app/models/classes/*"
      ],
      "@proxy-classes/*": [
        "src/app/models/proxy-classes/*"
      ],
      "@services/*": [
        "src/app/models/services/*"
      ],
      "@enums/*": [
        "src/app/models/enums/*"
      ],
      "@pipes/*": [
        "src/app/models/pipes/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@state/*": [
        "src/app/state/*"
      ],
      "@animations": [
        "src/app/app.animation.ts"
      ],
    },
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}